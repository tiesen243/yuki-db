{
  "name": "yuki-db",
  "version": "1.0.2",
  "description": "A client-side database querying library with React Query support",
  "keywords": [
    "react",
    "react-query",
    "database",
    "query",
    "client-side"
  ],
  "homepage": "https://yuki-ui.vercel.app/docs/yuki-db",
  "bugs": {
    "url": "https://github.com/tiesen243/yuki-db/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiesen243/yuki-db.git",
    "directory": "packages/yuki-db"
  },
  "license": "MIT",
  "author": {
    "name": "Tiesen",
    "email": "ttien56906@gmail.com",
    "url": "https://tiesen.id.vn"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/adapters/*.d.ts",
        "default": "./dist/adapters/*.js"
      },
      "require": {
        "types": "./dist/adapters/*.d.cts",
        "default": "./dist/adapters/*.cjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types.d.ts",
        "default": "./dist/types.js"
      },
      "require": {
        "types": "./dist/types.d.cts",
        "default": "./dist/types.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsdown",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsdown --watch",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@yuki/prettier-config",
  "devDependencies": {
    "@yuki/eslint-config": "workspace:*",
    "@yuki/prettier-config": "workspace:*",
    "@yuki/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsdown": "^0.13.2",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.84.1",
    "drizzle-orm": "^0.44.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
