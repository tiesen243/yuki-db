{
  "name": "yuki-db",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    "./drizzle": {
      "import": {
        "types": "./dist/drizzle/index.d.ts",
        "default": "./dist/drizzle/index.js"
      },
      "require": {
        "types": "./dist/drizzle/index.d.cts",
        "default": "./dist/drizzle/index.cjs"
      }
    },
    "./drizzle/client": {
      "import": {
        "types": "./dist/drizzle/client.d.ts",
        "default": "./dist/drizzle/client.js"
      },
      "require": {
        "types": "./dist/drizzle/client.d.cts",
        "default": "./dist/drizzle/client.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsdown --watch",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@yuki/prettier-config",
  "devDependencies": {
    "@yuki/eslint-config": "workspace:*",
    "@yuki/prettier-config": "workspace:*",
    "@yuki/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsdown": "^0.13.2",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.84.1"
  }
}
